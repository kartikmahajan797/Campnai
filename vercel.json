{
    "rewrites": [
        {
            "source": "/api/v1/:path*",
            "destination": "/server/api/index"
        },
        {
            "source": "/api/health",
            "destination": "/server/api/index"
        },
        {
            "source": "/api/:path*",
            "destination": "/server/api/index"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}