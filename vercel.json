{
    "rewrites": [
        {
            "source": "/api/v1/:path*",
            "destination": "/server/api/index"
        },
        {
            "source": "/api/health",
            "destination": "/server/api/index"
        },
        {
            "source": "/api/v1/:path*",
            "destination": "https://campnai.onrender.com/api/v1/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}